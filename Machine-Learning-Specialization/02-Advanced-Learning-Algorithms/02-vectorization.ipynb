    "FOR LOOPS"
   ]
  },
  {
   "cell_type": "raw",
   "id": "52237b2b-d707-4960-8120-c3753e2c0c2d",
   "metadata": {},
   "source": [
    "x = np.array([200, 17])\n",
    "\n",
    "W = np.array([[1, -3, 5],\n",
    "             [-2, 4, -6]])\n",
    "\n",
    "b = np.array([-1, 1, 2])\n",
    "\n",
    "def dense(a_in, W, b):\n",
    "    units = W.shape[1]\n",
    "    a_out = np.zeros(units)\n",
    "    for j in range(units):\n",
    "        w = W[:,j]\n",
    "        z = np.dot(w, a_in) + b[j]\n",
    "        a_out[j] = g(z)\n",
    "    return a_out\n",
    "\n",
    "dense(x, W, b)"
   ]
